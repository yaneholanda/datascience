# Importando as bibliotecas 
import pandas as pd
from pandas_datareader import data as web
import plotly.graph_objects as go

# Criando um DataFrame vazio
df = pd.DataFrame ()
# Um DataFrame é uma estrutura bidimensional de dados

# A ação escolhida foi da AMBEV
#Escrita padrão dos dados no site do Yahoo Finance
acao = 'ABEV3.SA'

#Importando os dados para o DataFrame
df = web.DataReader(acao, data_source='yahoo', start='01-01-2015')

#Mostrando as 5 primeiras linhas de dados
df.head()

#Plotando o gráfico Candlestick
trace1 = {
    'x': df.index,
    'open': df.Open,
    'close': df.Close,
    'high': df.High,
    'low': df.Low,
    'type': 'candlestick',
    'name': acao,
    'showlegend': False
}
#Informando todos os dados e gráficos em uma lista
data = [trace1]

#Configurando o layout do gráfico - adicionando um título
layout = go.Layout({
    'title':{
        'text':'Gráfico de Cotações - AMBEV'
        }
    })

#Instanciando o objeto Figure e plotando o gráfico
fig = go.Figure(data=data, layout=layout)
fig.show()
